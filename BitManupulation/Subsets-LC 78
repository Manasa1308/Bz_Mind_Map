class Solution {
    public List<List<Integer>> subsets(int[] a) {
        List<List<Integer>> r = new ArrayList<>();
        int n = a.length;
        for (int m = 0; m < (1 << n); m++) {
            List<Integer> t = new ArrayList<>();
            for (int i = 0; i < n; i++)
                if (((m >> i) & 1) == 1) t.add(a[i]);
            r.add(t);
        }
        return r;
    }
}
