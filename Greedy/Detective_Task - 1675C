import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while (t-- > 0) {
            String s = sc.next();
            int l = s.indexOf('1');
            int r = s.lastIndexOf('0');
            if (l == -1 || r == -1 || l > r) System.out.println(0);
            else System.out.println(r - l + 1);
        }
    }
}
